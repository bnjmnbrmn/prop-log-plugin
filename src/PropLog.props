# Properties file for PropLog

# jEdit only needs to load the plugin the first time the user accesses it
# the presence of this property also tells jEdit the plugin is using the new API
plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.activate=defer
#plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.activate=startup

# These two properties are required for all plugins
plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.name=PropLog
plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.author=Benjamin Berman

# description
plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.description=A Plugin for Propositional Logic

plugin.edu.uiowa.cs.baberman.proplogjedit.PropLogPlugin.version=0.1

